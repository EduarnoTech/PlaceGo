{
  "expo": {
    "name": "Tutorlancer",
    "slug": "Tutorlancer",
    "version": "2.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "plugins": [
      [
        "expo-image-picker",
        {
          "photosPermission": "The app accesses your photos to let you share them with your friends."
        }
      ]
    ],
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],

    "notification":{
      "icon":"./assets/push.png"
     }
     ,

    "ios": {
      "supportsTablet": false,
      "bundleIdentifier": "com.develop.tutorlancer",
      "infoPlist": {
        "NSCameraUsageDescription": "Please allow access to Camera to enable video conferencing.",
        "NSMicrophoneUsageDescription":"Please allow access to Microphone to enable video conferencing.",
        "NSLocalNetworkUsageDescription":"Please allow access to network usage to enable video conferencing"
      }
    },
    "android": {
      "package": "com.develop.tutorlancer",
      "googleServicesFile": "./google-services.json",
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#FFFFFF"
      },
      "permissions": [
        "android.permission.RECORD_AUDIO",
        "android.hardware.camera",
        "android.hardware.camera.autofocus",
        "android.permission.CAMERA",
        "android.permission.CHANGE_NETWORK_STATE",
        "android.permission.MODIFY_AUDIO_SETTINGS",
        "android.permission.RECORD_AUDIO",
        "android.permission.BLUETOOTH",
        "android.permission.INTERNET",
        "android.permission.ACCESS_NETWORK_STATE",
        "android.permission.FOREGROUND_SERVICE"
      ]
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "extra": {
      "eas": {
        "projectId": "da7a8ab1-7136-4fda-b694-29842d3a1f0a"
      }
    }
  }
}
